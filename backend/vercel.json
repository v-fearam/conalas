{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "https://conalas-frontend.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type"
            },
            "dest": "src/main.ts"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "headers": {
                "Access-Control-Allow-Origin": "https://conalas-frontend.vercel.app"
            }
        }
    ]
}
